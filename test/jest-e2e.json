{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/notion/(.*)": "<rootDir>/../libs/notion/src/$1",
    "@lib/notion": "<rootDir>/../libs/notion/src",
    "@lib/openai/(.*)": "<rootDir>/../libs/openai/src/$1",
    "@lib/openai": "<rootDir>/../libs/openai/src",
    "@lib/airtable/(.*)": "<rootDir>/../libs/airtable/src/$1",
    "@lib/airtable": "<rootDir>/../libs/airtable/src"
  }
}